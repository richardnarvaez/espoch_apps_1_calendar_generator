{
  "name": "zetplain",
  "description": "An awesome app!",
  "version": "0.2.55",
  "private": false,
  "homepage": "./",
  "main": "src/electron-starter.js",
  "author": "zeptplain <info@zetplain.com>",
  "license": "MIT",
  "build": {
    "appId": "zetplain.com.ec",
    "productName": "Zetplain",
    "asar": true,
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/favicon.ico"
    }
  },
  "dependencies": {
    "core-js": "^3.4.0",
    "electron-splashscreen": "^1.0.0",
    "firebase": "^5.11.1",
    "path": "^0.12.7",
    "react": "^16.11.0",
    "react-color": "^2.17.3",
    "react-datepicker": "^2.10.1",
    "react-dom": "^16.6.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.2.0",
    "recompose": "^0.30.0",
    "tui-calendar": "^1.12.11",
    "xlsx": "^0.15.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "electron": "electron . ",
    "pack-win": "electron-packager . ZetPlain --overwrite --platform  win32 --arch x64 --icon=build/favicon.ico --out dist/",
    "p": "electron-installer-windows --src dist/app-win32-x64/ --dest dist/installers/",
    "pack-mac": "electron-packager .",
    "build": "react-scripts build",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "test": "react-scripts test --env=jsdom --passWithNoTests",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^13.6.6",
    "electron-builder": "^22.3.2",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^14.2.0",
    "electron-reload": "^1.5.0",
    "electron-wix-msi": "^2.2.0",
    "webpack": "^4.41.0"
  }
}
